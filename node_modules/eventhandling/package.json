		"babel-register": ">=6.7.2",
		"eslint": "^3.0.1",
		"eslint-plugin-babel": ">=3.3.0",
		"eslint-plugin-flowtype": ">=2.7.1",
		"rollup": "^0.34.10",
		"rollup-plugin-babel": "^2.6.1",
		"rollup-plugin-commonjs": "^3.3.1",
		"rollup-plugin-filesize": "^0.4.4",
